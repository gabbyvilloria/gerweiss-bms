#batteryEncoding.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
    .modal-dialog.modal-lg(role='document')
        .modal-content
            .modal-header
                h5#exampleModalLabel.modal-title Battery Encoding
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') Ã—
            .modal-body
                .container
                    .row
                        .col-3
                            legend Transaction
                            .form-check.mb-1
                                label.form-check-label
                                    input#optionsRadios1.form-check-input(type='radio', name='optionsRadios', value='option1', checked='')
                                    |           UNIT IN
                            .form-check.mb-1
                                label.form-check-label
                                    input#optionsRadios2.form-check-input(type='radio', name='optionsRadios', value='option2')
                                    |           UNIT OUT
                            .form-check.mb-1
                                label.form-check-label
                                    input#optionsRadios3.form-check-input(type='radio', name='optionsRadios', value='option3')
                                    |           Charge IN
                            .form-check.mb-1
                                label.form-check-label
                                    input#optionsRadios4.form-check-input(type='radio', name='optionsRadios', value='option4')
                                    |           Charge OUT
                        .col
                            legend Battery Code
                            .form-group
                                label(for='exampleSelect1') Select battery
                                select#exampleSelect1.form-control
                                    option N01
                                    option N02
                                    option N03
                                    option N04
                                    option N05
                    .row
                        .col

            .modal-footer
                button.btn.btn-info(type='button', data-dismiss='modal') Close
                button.btn.btn-success(type='button') Submit